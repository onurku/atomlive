{
  "name": "atom-frontend",
  "version": "1.0.0",
  "license": "MIT",
  "engines": {
    "node": "18.17.0"
  },
  "scripts": {
    "dev": "set NODE_OPTIONS=--openssl-legacy-provider && set DB_HOST=127.0.0.1 && next dev",
    "build": "set NODE_OPTIONS=--openssl-legacy-provider next build",
    "buildlocal": "NODE_OPTIONS=--openssl-legacy-provider next build",
    "start": "set NODE_OPTIONS=--openssl-legacy-provider next start",
    "dblocal:migrate": "npx sequelize-cli db:migrate --env 'development'",
    "db:migrate": "npx sequelize-cli db:migrate --env 'production'",
    "stripe:listen": "stripe listen --forward-to=localhost:4000/api/webhooks --project-name=saas-starter",
    "cy:open-only": "cypress open",
    "cy:run-only": "cypress run",
    "cy:open": "start-server-and-test dev 3000 cy:open-only",
    "cy:run": "start-server-and-test dev 3000 cy:run-only",
    "cy:run:chrome": "cypress run --browser chrome",
    "cy:run:firefox": "cypress run --browser firefox",
    "tests": "percy exec -- cypress run"
  },
  "dependencies": {
    "@aws-amplify/adapter-nextjs": "^1.0.28",
